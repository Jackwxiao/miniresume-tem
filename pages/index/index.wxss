
.photo{
  display: block;width:100%;
  position: fixed;bottom: 0;
}
.cover-info{
  position: absolute;left: 50px;top: 30px;
}
.cover-name{
  font-size: 30px;font-weight: bold;display: block;
  letter-spacing: 1px;color: #000;
}
.cover-spell{
  font-size: 20px;letter-spacing: 1px;
}
.big-circle{
  background-color: #00b26a;
  width: 240px;height: 240px;
  border-radius: 120px;opacity: 0.5;
  position:absolute;left: -150px;top: 100px;
}
.medium-circle{
  background-color: #00b26a;
  width: 140px;height:140px;
  border-radius: 70px;
  position:absolute;right: -70px;top: 110px;
}
.small-circle{
  background-color: #00b26a;
  width: 50px;height:50px;
  border-radius: 25px;
  position:absolute;right: -10px;bottom: 110px;
}
.cover-work{
  position: absolute;
  bottom: 180px;opacity: 0.8;
  text-align: center;
  background-color: pink;
  padding: 20px 5px;width: 100%;
}
.cover-job{
  display: block;color: #fff;font-size: 30px;font-weight: bold
}
.navgation{
  position: absolute; top: 40px; right: 10px; font-size: 18px;
}
.right{
  width: 20px;height: 20px;vertical-align: -4px;
}
.section{
  padding-bottom: 30px;
}
.section-info{
  display: flex;
  border-bottom: 1px solid #333;padding-bottom:5px;
  margin: 10px 0;
}
.section-seq{
  background-color: #444;
  width: 22px;
  height: 22px;color: #fff;text-align: center;line-height: 22px;margin-right: 10px;font-weight: bold;margin-top:2px;
}
.section-title{
 font-size: 20px;
}
.column{
  font-size: 16px;border-bottom: dashed 1px #333;
  padding: 10px 0;
}
.col-2{
  column-count: 2;
}
.col-1{
  column-count: 1;
}
.info{
  display: block;
}
.skill{
  padding: 5px 0;
}
.skill-name{
  font-size: 16px;display: block;margin-bottom: 5px;
}
.edu{
  border-bottom: dashed 1px #999;
  padding: 10px 0;
}
.edu:last-of-type{
  border-bottom: none;
}
.school{
  font-size: 18px;
  margin-bottom: 10px;
}
.icon-school{
  width: 26px;height: 26px;vertical-align: -6px;margin-right: 5px;
}
.row{
  display: flex;justify-content: space-between;font-size: 16px;
}
.info{
  margin-bottom: 10px;
}
.icon-major{
  width: 16px;height: 16px;vertical-align: -2px;margin-right: 5px;
}
.desc{
  color: #999;
  line-height: 1.5;
}
.page{
  display: block; width: 100%; text-align: center;font-size: 16px;padding-bottom: 15px;
}
.resume{
  margin: 0 10px;
}